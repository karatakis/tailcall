---
source: tests/core/spec.rs
expression: formatter
---
schema @server @upstream {
  query: Query
}

union Role = Admin | User

type Admin {
  name: String!
  region: String!
}

type Query {
  users: [[Role!]!]! @http(url: "http://upstream/users")
}

type User {
  accountRef: String! @http(url: "http://upstream/refs/{{.value.id}}")
  id: ID!
  name: String!
}
